<%= form_for property,url: propertys_new_or_edit_path do |f| %>

  <div class="field">
    <%= f.label :物件名 %><br>
    <%= f.text_field :name, id: :property_name%>
  </div>

  <div class="field">
    <%= f.label :賃料 %><br>
    <%= f.text_field :rent, id: :property_rent%>
    <%= "円" %>
  </div>

  <div class="field">
    <%= f.label :住所 %><br>
    <%= f.text_field :address, id: :property_address%>
  </div>

  <div class="field">
    <%= f.label :築年数 %><br>
    <%= f.text_field :age, id: :property_age%>
    <%= "年" %>
  </div>

  <div class="field">
    <%= f.label :備考 %><br>
    <%= f.text_area :remarks, id: :property_remarks%>
  </div>

  <hr>

  <h3>最寄り駅1</h3>
  <div class="field">
    <%= f.label :路線名 %><br>
    <%#= f.text_area :route, id: :station_route%>
  </div>

  <div class="field">
    <%= f.label :駅名 %><br>
    <%#= f.text_area :name, id: :station_name%>
  </div>

  <div class="field">
    <%= f.label :徒歩分数 %><br>
    <%#= f.text_area :toho, id: :station_toho%>
  </div>

  <h3>最寄り駅2</h3>

  <div class="actions">
    <%= f.submit '登録する' %>
  </div>

<% end %>

<%= link_to '戻る', propertys_path %>